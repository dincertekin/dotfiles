{
  "agent": {
    "default_profile": "ask",
    "default_model": {
      "provider": "google",
      "model": "gemini-2.0-flash"
    },
    "model_parameters": []
  },
  "autosave": "on_focus_change",
  "format_on_save": "on",
  "soft_wrap": "bounded",
  "disable_ai": false,
  "vim_mode": false,
  "minimap": {
    "show": "auto"
  },
  "agent_buffer_font_size": 14,
  "ui_font_family": ".SystemUIFont",
  "ui_font_size": 16,
  "buffer_font_family": "Cascadia Code",
  "buffer_font_size": 18,
  "buffer_font_features": {
    "calt": true
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "tabs": {
    "file_icons": true
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
